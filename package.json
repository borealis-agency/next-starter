{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "start": "next start",
    "lint:js": "next lint",
    "lint:js:report": "eslint --output-file eslint_report.json --format json",
    "lint:styles": "npx stylelint '**/*.{css,scss,sass}'",
    "prepare": "husky install",
    "packagecheck": "npx --yes package-changed",
    "dev:next": "npm run packagecheck && next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "dev": "concurrently \"npm:dev:*\""
  },
  "dependencies": {
    "axios": "0.27.2",
    "next": "12.2.0",
    "normalize.css": "8.0.1",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.30.7"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "concurrently": "7.3.0",
    "cross-env": "7.0.3",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "glob": "8.0.3",
    "husky": "8.0.1",
    "hygen": "6.2.8",
    "lint-staged": "13.0.3",
    "pathpida": "0.18.1",
    "prettier": "2.7.1",
    "sass": "1.53.0",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-standard-scss": "4.0.0",
    "svg-sprite": "1.5.4",
    "typescript": "4.7.4"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.13.2"
  }
}
