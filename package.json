{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "start": "next start",
    "lint:js": "next lint",
    "lint:js:report": "eslint --output-file eslint_report.json --format json",
    "lint:styles": "npx stylelint \"**/*.{css,scss,sass}\"",
    "prepare": "husky install",
    "packagecheck": "npx --yes package-changed",
    "dev:next": "npm run packagecheck && next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "dev": "concurrently \"npm:dev:*\"",
    "generate:iconsprite": "node scripts/generateIconSprite.mjs"
  },
  "dependencies": {
    "axios": "1.2.0",
    "next": "13.0.5",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.31.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "glob": "8.0.3",
    "husky": "8.0.2",
    "hygen": "6.2.11",
    "lint-staged": "13.0.4",
    "pathpida": "0.20.0",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.0",
    "sass": "1.56.1",
    "stylelint": "14.15.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "svg-sprite": "2.0.2",
    "typescript": "4.9.3",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.13.2"
  }
}
