{
	"name": "next-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "npm run generate:paths && next build",
		"start": "next start",
		"lint:js": "npx eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./src",
		"lint:js:report": "npm run lint:js -- --output-file eslint_report.json --format json",
		"packagecheck": "npx --yes package-changed@3.0.0",
		"dev:next": "next dev --turbo",
		"dev:path": "npm run generate:paths -- --watch",
		"dev": "npm run packagecheck && concurrently \"npm:dev:*\"",
		"generate:paths": "pathpida --enableStatic --ignorePath .gitignore --output ./src/paths",
		"generate:icons": "node scripts/generateIconSprite.mjs && npm run generate:paths",
		"generate:api": "npx --yes swagger-typescript-api@12.0.4 --axios -r --module-name-index 1 -p __SWAGGER_JSON_URL_HERE__ -o ./src/api -n api-generated.ts --templates ./swaggerGenerateTemplates && npx hygen api client"
	},
	"dependencies": {
		"axios": "1.6.8",
		"classnames": "2.3.2",
		"next": "14.2.3",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"sharp": "0.33.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.2",
		"@types/node": "20.12.7",
		"@types/react": "18.3.1",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "6.16.0",
		"@typescript-eslint/parser": "6.16.0",
		"autoprefixer": "10.4.19",
		"concurrently": "8.2.2",
		"cross-env": "7.0.3",
		"eslint": "8.52.0",
		"eslint-config-next": "14.2.3",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-plugin-import": "2.29.0",
		"glob": "8.1.0",
		"hygen": "6.2.11",
		"lefthook": "1.6.10",
		"package-changed": "3.0.0",
		"pathpida": "0.22.0",
		"postcss": "8.4.38",
		"svg-sprite": "2.0.4",
		"tailwindcss": "3.4.3",
		"typescript": "5.4.5"
	},
	"volta": {
		"node": "20.12.2",
		"npm": "10.6.0"
	}
}
