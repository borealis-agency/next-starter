{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run packagecheck && pathpida --enableStatic --ignorePath .gitignore && next build",
    "start": "next start",
    "lint:js": "npx eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./",
    "lint:js:report": "npm run lint:js -- --output-file eslint_report.json --format json",
    "lint:styles": "npx stylelint \"**/*.{css,scss,sass}\"",
    "packagecheck": "npx --yes package-changed@3.0.0",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "dev": "npm run packagecheck && concurrently \"npm:dev:*\"",
    "generate:icons": "node scripts/generateIconSprite.mjs && pathpida --enableStatic --ignorePath .gitignore",
    "generate:api": "npx --yes swagger-typescript-api@12.0.4 --axios -r --module-name-index 1 -p __SWAGGER_JSON_URL_HERE__ -o ./api -n api-generated.ts --templates ./swaggerGenerateTemplates"
  },
  "dependencies": {
    "axios": "1.3.5",
    "classnames": "2.3.2",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "concurrently": "8.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "glob": "8.1.0",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "lint-staged": "13.2.1",
    "pathpida": "0.20.1",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.7",
    "sass": "1.61.0",
    "stylelint": "15.4.0",
    "stylelint-config-standard": "32.0.0",
    "stylelint-config-standard-scss": "8.0.0",
    "svg-sprite": "2.0.2",
    "typescript": "5.0.4",
    "typescript-plugin-css-modules": "5.0.1"
  },
  "volta": {
    "node": "18.15.0",
    "npm": "9.6.4"
  }
}
